void principal() {
    
}

const edad := 10;

void funcionIf() {
    string one = "correcto, ya puedes ir a la carcel";
    string two = "incorrecto";
    string name = "Javier";
    int edad = 24;
    if (edad == 18) {
        print(one);
    }
    else if (edad == 21) {
        print("ya puedes consumir alcohol en todo el mundo");
    }
    else if (edad == 30) {
        print("Ya no hay vuelta atras amigo");
    }
    else {
        print("Esta edad no es tan significativa meh\n");
        if (name == "Javier") {
            print("Aun puedes salir de esta, yo creo en ti");
        }
        else {
            print("No te conozco, pero se que puedes");
        }
    }
}

void emptyReturn() {
    boolean b = true
    if (b) {
        print("Ejecutando el principio\n");
    }

    if (edad > 10) {
        print("Efectivamente\n")
        return;
    }

    print("Hmmm, no lo se amiguito");
}

void funcionWhile() {
    int i = 0;
    while (i < 100) {
        if (i == 15) {
            i++
            continue
        }
        else if (i == 30) {
            break
        }
        else if (i == 26) {
            print("retornando sin hacer break");
            return;
        }
        print(i);
        print('\n');
        i++
    }
    print("aplicando break\n");
}

void funcionSwitch() {
    int a = 8;
    switch(a) {
        case 6:
            print("aqui no es - 6");
            break;
        case 7:
            print("aqui tampoco es - 7")
            break
        case 8:
            print("aqui si es - 8");
            break;
        default:
            print("aqui menos es - default");
    }

    string b = "javs" 
    switch (b) {
        case "javs":
        case "javier":
            print("i can, i will, i must");
            break;
        case "josue":
            print("he can, he will, he must");
            break;
    }

    string c = "geperipigoponsapa";
    switch(c) {
        case "gep":
            print("aqui no era");
            break;
        case "perior":
            print("aqui tampoco es");
        default:
            print("aqui si es");
    }
}